    resource:
      aws_nat_gateway:
        {{ name }}:
          allocation_id: '${var.{{ name }}_allocation_id}'
          subnet_id: '${var.{{ name }}_subnet_id}'
          # tags:
          #   '[TO_BE_REPLACED]': ''
    variable:
      {{ name }}_allocation_id:
        type: 'string'
      {{ name }}_subnet_id:
        type: 'string'
    output:
      id:
        value: '${aws_nat_gateway.{{ name }}.id}'
      thub_id:
        value: '${aws_nat_gateway.{{ name }}.id}'
      network_interface_id:
        value: '${aws_nat_gateway.{{ name }}.network_interface_id}'
      private_ip:
        value: '${aws_nat_gateway.{{ name }}.private_ip}'
      public_ip:
        value: '${aws_nat_gateway.{{ name }}.public_ip}'

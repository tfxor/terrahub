  template:
    # terraform:
    #   backend:
    #     bucket: 'tf-state-prod'
    #     prefix: 'terraform/state'
    resource:
      google_folder:
        {{ name }}:
          parent: '${var.{{ name }}_parent}'
          display_name: '${var.{{ name }}_display_name}'
    variable:
      {{ name }}_parent:
        type: 'string'
      {{ name }}_display_name:
        type: 'string'
    output:
      id:
        value: '${google_folder.{{ name }}.id}'
      thub_id:
        value: '${google_folder.{{ name }}.id}'
      name:
        value: '${google_folder.{{ name }}.name}'
      lifecycle_state:
        value: '${google_folder.{{ name }}.lifecycle_state}'
      create_time:
        value: '${google_folder.{{ name }}.create_time}'
